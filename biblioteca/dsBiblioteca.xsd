<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsBiblioteca" targetNamespace="http://tempuri.org/dsBiblioteca.xsd" elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/dsBiblioteca.xsd" xmlns:mstns="http://tempuri.org/dsBiblioteca.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsBiblioteca" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Tema">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Cod_Tema" type="xs:int" />
							<xs:element name="Descripcion" type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Libro">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Cod_Lib" type="xs:int" />
							<xs:element name="Tit_Lib" type="xs:string" />
							<xs:element name="Aut_Lib" type="xs:string" />
							<xs:element name="NoPag_Lib" type="xs:int" />
							<xs:element name="Edit_Lib" type="xs:string" />
							<xs:element name="NoEdic_Lib" type="xs:string" />
							<xs:element name="FechaIngreso" type="xs:dateTime" />
							<xs:element name="Docto_Ing" type="xs:string" minOccurs="0" />
							<xs:element name="Cod_Tema" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="dsBibliotecaKey1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Tema" />
			<xs:field xpath="mstns:Cod_Tema" />
		</xs:unique>
		<xs:unique name="dsBibliotecaKey2" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Libro" />
			<xs:field xpath="mstns:Cod_Lib" />
		</xs:unique>
		<xs:key name="key1">
			<xs:selector xpath=".//mstns:Libro" />
			<xs:field xpath="mstns:Cod_Tema" />
		</xs:key>
		<xs:keyref name="LibroTema" refer="key1">
			<xs:selector xpath=".//mstns:Tema" />
			<xs:field xpath="mstns:Cod_Tema" />
		</xs:keyref>
	</xs:element>
</xs:schema>